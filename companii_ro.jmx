            <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor" enabled="true">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">// Retrieve the value from the CSV using the variable placeholder
String rawStareFirma = vars.get(&quot;STARE_FIRMA&quot;)

// Split if there are multiple values in STARE_FIRMA
List&lt;String&gt; codes = rawStareFirma.split(&apos;,&apos;)

// Map each code to its description using JMeter variables
List&lt;String&gt; descriptions = codes.collect { code -&gt;
    String description = vars.get(&quot;stare_&quot; + code.trim())
    description ?: &quot;Unknown status code: $code&quot;  // Fallback for unknown codes
}

// Join descriptions into a JSON-ready format
String jsonFormattedDescriptions = descriptions.collect { &quot;\&quot;$it\&quot;&quot; }.join(&apos;, &apos;)

// Store the formatted descriptions back into a variable for use in HTTP request
vars.put(&quot;jsonStareDescriptions&quot;, jsonFormattedDescriptions)</stringProp>
            </JSR223PreProcessor>
