            <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor" enabled="true">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">// Assuming &apos;STARE_FIRMA&apos; is the variable that holds the value &quot;1074,2120&quot; read from the CSV
String codStare = vars.get(&quot;STARE_FIRMA&quot;)
// Split the string by comma to handle multiple values
List&lt;String&gt; codStareList = codStare.split(&quot;,&quot;);
// Convert list to JSON array representation
String jsonArray = codStareList.collect { &quot;\&quot;&quot; + it.trim() + &quot;\&quot;&quot; }.join(&quot;, &quot;)

// Put the jsonArray back into a variable
vars.put(&quot;jsonFormattedCodStare&quot;, jsonArray)</stringProp>
            </JSR223PreProcessor>
